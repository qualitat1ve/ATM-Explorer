    <!--<SQL raw queries START>-->
    <string name="sql_select_all">SELECT Atm._id, Bank.bank_name, City.name, Atm.address, Bank.logo, Atm.latitude,
        Atm.time, Atm.position, Atm.description, Atm.longitude FROM Atm INNER JOIN Bank ON Bank._id=Atm.id_bank
        INNER JOIN City ON City._id=Atm.id_city</string>
    <string name="sql_filter_by_address">SELECT Atm._id, Bank.bank_name, City.name, Atm.address, Bank.logo,
        Atm.latitude, Atm.longitude, Atm.time, Atm.position, Atm.description FROM Atm INNER JOIN Bank ON
        Bank._id=Atm.id_bank INNER JOIN City
        ON City._id=Atm.id_city WHERE Atm.address LIKE ?</string>
    <!--<SQL raw queries END>-->

